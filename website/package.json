  "type": "module",
  "main": "src/main.ts",
  "scripts": {
    "dev": "next dev",
    "build": "pnpm run lint && pnpm run test && pnpm run esbuild:lambda && next build -d",
    "start": "pnpm run build && pnpm serve out",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "sam-start-api": "export $(grep -v '^#' .env.local | xargs) && pnpm esbuild:lambda && sam local start-api -p 3001",
    "sam-invoke-presign": "pnpm run build && sam local invoke PresignFunction --event events/event.json",
    "esbuild:lambda": "node --trace-deprecation esbuild.config.mjs",
    "test": "node --trace-deprecation ./node_modules/jest/bin/jest.js  --config jest.config.ts --coverage --passWithNoTests",
    "test:watch": "node --trace-deprecation ./node_modules/jest/bin/jest.js  --config jest.config.ts --watch --coverage",
    "type": "tsc --noEmit",
    "nuke": "rm -rf .next coverage out node_modules pnpm-lock.yaml"
  },
